page {
    width: 100%;
    height: 100%;
    background: #f0f0f0;
    padding-top: 140rpx;
    box-sizing: border-box;
}

.Withdrawaltitle {
    width: 100%;
    height: 190rpx;
    padding-left: 30rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    background: #fff;
}

.Withdrawaltitle image {
    width: 60rpx;
    height: 60rpx;
}

.Withdrawaltitle text {
    color: #000000;
    font-size: 36rpx;
    margin-left: 20rpx;
}

.Withdrawal_view {
    padding: 50rpx 40rpx;
    box-sizing: border-box;
    height: 347rpx;
    width: 100%;
    display: flex;
    justify-content: space-between;
    flex-direction: column;
    background: #fff;
    margin-top: 20rpx;
}

.tiana {
    height: 115rpx;
    width: 100%;
    position: relative;
    align-items: center;
    justify-content: center;
}

.tiana text {
    color: #333333;
    font-size: 50rpx;
    position: absolute;
    top: 25rpx;
    left: 0;
}

.Withdrawal_input {
    width: 100%;
    align-items: center;
    height: 100%;
    padding-left: 44rpx;
    box-sizing: border-box;
    border-bottom: 2rpx solid #eeeeee;
}

.current {
}

.current text {
    color: #888888;
    font-size: 25rpx;
    line-height: 30rpx;
}

.current .redcurrent {
    color: #f30d0d;
    font-size: 25rpx;
}

.drawal {
    width: 100%;
    padding: 0 40rpx;
    box-sizing: border-box;
    background: #fff;
    padding-top: 30rpx;
    margin-top: 15rpx;
}

.drawal_twu {
    width: 100%;
    height: 70rpx;
    display: flex;
    color: #333333;
    font-size: 26rpx;
    align-items: center;
    border-bottom: 2rpx solid #f5f5f5;
}

.wx_cku {
    width: 53rpx;
    height: 56rpx;
}

.drawal_vu {
    width: 100%;
    padding: 0 18rpx;
    box-sizing: border-box;
    height: 112rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.drawal_vu text {
    color: #666666;
    font-size: 26rpx;
    margin-right: auto;
    margin-left: 15rpx;
}

.wx_cksol {
    width: 30rpx;
    height: 30rpx;
}

.Withdrbtn {
    width: 90%;
    margin: 0 auto;
    height: 82rpx;
    border-radius: 15rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 85rpx;
}