<imperial backgroundColor="{{backgroundColor}}" bind:disappear="listdisappear" id="imperial" title="提现"></imperial>
<form bindreset="formReset" bindsubmit="formSubmit">
    <view class="Withdrawal_view">
        <text>提现金额</text>
        <view class="tiana">
            <text>￥</text>
            <input class="Withdrawal_input" name="tmoney" placeholder="0"></input>
        </view>
        <view class="current">
            <text>当前余额：</text>
            <text class="redcurrent">￥{{treemoney}}</text>
        </view>
    </view>
    <view class="drawal">
        <text class="drawal_twu">提现方式</text>
        <view class="drawal_vu">
            <image class="wx_cku" src="../../images/wx-ch.png"></image>
            <text>微信</text>
            <image class="wx_cksol" src="../../images/tryue.png"></image>
        </view>
    </view>
    <button class="Withdrbtn" formType="submit" type="primary">提现</button>
</form>
