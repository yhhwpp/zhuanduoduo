<imperial backgroundColor="{{backgroundColor}}" bind:disappear="listdisappear" id="imperial" title="联系我们"></imperial>
<view class="lianxiwon">
    <image src="../../resource/images/1803240850211_02.png"></image>
</view>
<view class="main">
    <view class="main_text">
        <text>{{ofFriendsimg.contact}}</text>
    </view>
    <view bindtap="imgYu" class="erwei" data-src="{{ofFriendsimg.contact_qr}}">
        <image class="erweibord" src="../../resource/images/sao_03.png"></image>
        <image bindlongtap="bingLongTap" bindtap="imgYu" class="erwi" data-src="{{ofFriendsimg.contact_qr}}" src="{{ofFriendsimg.contact_qr}}"></image>
    </view>
    <view class="changan">
        <image src="../../resource/images/acvadvds.png"></image>
        <text bindlongtap="bingLongTap">点击图片可保存二维码</text>
    </view>
</view>
