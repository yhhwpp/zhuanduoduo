page {
    width: 100%;
    height: 100%;
}

.lianxiwon {
    width: 100%;
    height: 435rpx;
}

.lianxiwon image {
    width: 100%;
    height: 100%;
    margin-top: 70rpx;
}

.main {
    display: flex;
    align-items: center;
    flex-direction: column;
    margin-top: 70rpx;
}

.main_text {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.main_text text {
    color: #25cbfb;
    font-size: 36rpx;
}

.main_text text:first-child {
    color: #000000;
    font-size: 33rpx;
}

.erwei {
    width: 320rpx;
    height: 320rpx;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 108rpx;
}

.erweibord {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 0;
}

.erwi {
    width: 98%;
    height: 98%;
    z-index: 1;
}

.changan {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 49rpx;
}

.changan image {
    width: 35rpx;
    height: 35rpx;
    margin-right: 15rpx;
}

.changan text {
    color: #010101;
    font-size: 26rpx;
}