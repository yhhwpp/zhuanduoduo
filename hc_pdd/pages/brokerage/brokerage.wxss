.tixian {
    position: relative;
    height: 385rpx;
}

.tiding {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 0;
}

.tiding_te {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    padding: 0 40rpx;
    box-sizing: border-box;
    z-index: 1;
}

button {
    padding: 0;
    margin: 0;
    border-radius: 0;
    background: none;
}

button::after {
    content: none;
}

.ketyixi {
    font-size: 35rpx;
    color: #fff;
}

.keyong {
    width: 100%;
    text-align: center;
    color: #edf6ff;
    margin-top: 48rpx;
    font-size: 83rpx;
    display: block;
}

.yif {
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin-top: 50rpx;
}

.yif_one {
    width: 48%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.yif_one text {
    color: #fff;
}

.yif .yif_one:first-child {
    position: relative;
}

.yif .yif_one:first-child::before {
    content: "";
    width: 0;
    height: 63rpx;
    position: absolute;
    border-right: 2rpx solid #fff;
    top: 20rpx;
    right: 0rpx;
}

.zhu {
    width: 100%;
    padding: 0 34rpx;
    box-sizing: border-box;
    line-height: 80rpx;
    font-size: 27rpx;
    color: #343434;
}

.irra {
    width: 100%;
    height: 10rpx;
    background: #e8e8e8;
}

.ciew_zhid {
    width: 100%;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.shur {
    width: 100%;
    height: 97rpx;
    display: flex;
    align-items: center;
    border-bottom: 2rpx solid #eee;
}

.wxchat {
    color: #343434;
    font-size: 30rpx;
    display: flex;
    width: 120rpx;
}

.fvhs {
    color: #c3c3c3;
    font-size: 33rpx;
}

.tisxhav {
    width: 250rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-right: 20rpx;
}

.tisxhav text {
    color: #000;
    font-size: 30rpx;
}

.tixiasn {
    width: 100%;
    height: 87rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #fff;
    font-size: 30rpx;
    background: #000;
    border-radius: 10rpx;
    margin-top: 87rpx;
}