page {
    width: 100%;
    height: 100%;
    background: #e3d0a9;
}

.toubu {
    width: 100%;
    height: 733rpx;
    position: relative;
}

.toubu_img {
    width: 100%;
    height: 100%;
}

button {
    padding: 0;
    margin: 0;
    border-radius: 0;
    background: none;
    line-height: normal;
}

button::after {
    content: none;
}

.soso {
    width: 80%;
    height: 85rpx;
    position: absolute;
    bottom: 142rpx;
    border-radius: 50rpx;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 137rpx;
    left: 10%;
}

.sosobut {
    width: 617rpx;
    height: 70rpx;
    display: flex;
    align-items: center;
    padding: 0 80rpx;
    justify-content: space-between;
    position: relative;
}

.soso_img {
    width: 44rpx;
    height: 44rpx;
    position: absolute;
    top: 20rpx;
    left: 50rpx;
}

.soso text {
    color: #999999;
    font-size: 26rpx;
}

.madfshgimg {
    width: 100%;
    display: block;
    height: 4000rpx;
    margin-top: 440rpx;
}

.sosopin {
    width: 100%;
    display: flex;
    box-sizing: border-box;
    justify-content: center;
    position: absolute;
    bottom: 24rpx;
    z-index: 11;
}

.sosopin_view {
    width: 275rpx;
    border-radius: 19rpx;
    height: 75rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
    box-shadow: 2px 5px 20px #333333;
    margin: 0 30rpx;
}

.sosopin_view image {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.sosopin_view text {
    color: #a47d53;
    font-size: 32rpx;
    position: relative;
}

.sosopin_bord {
    position: relative;
}

.sosopin_bord::before {
    content: '';
    width: 275rpx;
    height: 76rpx;
    position: absolute;
    top: -6rpx;
    left: -4rpx;
    border-radius: 19rpx;
    border: 2px solid #caa656;
}

.sosopin_titleimg {
    width: 100%;
    display: block;
    height: 169rpx;
    position: relative;
    z-index: 2;
}

.sosopin_img {
    margin-top: -42rpx;
    z-index: 10;
    position: relative;
}

.sosopin_titleimg_to {
    margin: 0;
    padding: 0;
    width: 660rpx;
    margin: 0 auto;
    display: block;
    z-index: 1;
    position: relative;
    background: #fff;
}

.sosopin_titleimg_to::before {
    content: '';
    position: absolute;
    height: 10px;
    width: 100%;
    background: #fff;
    top: 0px;
    left: 0;
    z-index: 10;
}

.sosopin_img_view {
    padding-bottom: 82rpx;
    background: #525680;
    margin-top: -1px;
}

.sosobut image {
    width: 40rpx;
    height: 40rpx;
}

.sosobut input {
    color: #999999;
    font-size: 26rpx;
    width: 85%;
    text-align: left;
}