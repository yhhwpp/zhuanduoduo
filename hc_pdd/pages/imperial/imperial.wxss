.statusBarHeight {
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    display: flex;
    height: 72px;
    align-items: center;
    color: #fff;
    z-index: 11;
}

.statusBarHeight text {
    font-size: 33rpx;
    color: #fff;
    margin-left: 0;
}

.left_imga {
    width: 70rpx;
    height: 70rpx;
}

.buttonum {
    height: 44px;
    margin-top: 28px;
    width: 100%;
    display: flex;
    align-items: center;
}

.buttonum_ui {
    height: 100%;
    display: flex;
    align-items: center;
}

.buttonum_uicl {
    background: rgba(0,0,0,0.2);
}

button {
    padding: 0;
    margin: 0;
    border-radius: 0;
    background: none;
    line-height: normal;
}

button::after {
    content: none;
}

.button-hover {
    background-color: none;
}