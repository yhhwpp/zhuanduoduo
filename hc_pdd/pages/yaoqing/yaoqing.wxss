page {
    width: 100%;
    height: 100%;
}

.yaqing {
    width: 100%;
    height: 100%;
    position: relative;
}

.qingimg {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    z-index: -1;
    left: 0;
}

.baohun {
    width: 100%;
    height: 88rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 10rpx;
}

.gdzltz {
    width: 183rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    position: relative;
}

.gdzltz::before {
    position: absolute;
    content: "\200b";
    width: 177rpx;
    height: 2rpx;
    background: #f44e4e;
    top: 42rpx;
    left: -200rpx;
}

.gdzltz::after {
    position: absolute;
    content: "\200b";
    width: 177rpx;
    height: 2rpx;
    background: #f44e4e;
    top: 42rpx;
    right: -200rpx;
}

.gdzltz image {
    width: 53rpx;
    height: 53rpx;
}

.gdzltz text {
    color: #ff5800;
    font-size: 37rpx;
}

.gdzltz text:first-child {
    color: #a8a8a8;
    font-size: 26rpx;
}

.sjacbA {
    width: 100%;
    position: absolute;
    display: flex;
    justify-content: center;
    flex-direction: column;
    top: 37%;
    align-items: center;
    left: 0;
}

.loging {
    width: 578rpx;
    height: 70rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 221rpx;
    color: #fff;
    font-size: 26rpx;
    border-radius: 15rpx;
    background: linear-gradient(to right,#ff8a08,#ff5700);
}

.ahjvdsi {
    width: 578rpx;
    height: 70rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 26rpx;
    color: #ff6211;
    font-size: 26rpx;
